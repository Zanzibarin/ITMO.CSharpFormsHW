<Window x:Class="ITMO.PAI.WpfHello.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ITMO.PAI.WpfHello"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Canvas>
        <Button x:Name="button" Content=" Set Name" HorizontalAlignment="Left" Margin="63,58,0,0" VerticalAlignment="Top" Width="87" Click="Button_Click"/>
        <Button x:Name="button1" Content="Ret Name" HorizontalAlignment="Left" Margin="63,112,0,0" VerticalAlignment="Top" Width="87" Click="Button_Click_1"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Margin="261,60,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="378" TextChanged="textBox_TextChanged"/>
        <Label x:Name="label" Content="" HorizontalAlignment="Left" Margin="259,109,0,0" VerticalAlignment="Top" Width="380"/>
        <Button Canvas.Left="10" Canvas.Top="10" Content="Click Me" Height="23" Name="button3" Width="75"  Click="button3_Click" />
        <TextBox Canvas.Left="90" Canvas.Top="12" Width="170" x:Name="myTextBox"/>
    </Canvas>
</Window>
